        <deprecated>false</deprecated>
        <description>Amount of additional discount on this quote.</description>
        <externalId>false</externalId>
        <formula>SBQQ__RegularAmount__c - SBQQ__CustomerAmount__c</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
