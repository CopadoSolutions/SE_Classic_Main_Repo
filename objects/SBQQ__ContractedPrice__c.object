    <validationRules>
        <fullName>SBQQ__Discount_or_price_required</fullName>
        <active>true</active>
        <description>Requires either Discount, Price, or Discount Schedule be entered.</description>
        <errorConditionFormula>AND(ISBLANK(SBQQ__Discount__c),ISBLANK(SBQQ__Price__c), ISBLANK(SBQQ__DiscountSchedule__c))</errorConditionFormula>
        <errorMessage>You must enter either Discount, Price, or Discount Schedule.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SBQQ__Either_product_or_filter</fullName>
        <active>true</active>
        <description>Either product or filter must be specified; not both.</description>
        <errorConditionFormula>AND(NOT(ISBLANK(SBQQ__Product__c)),NOT(ISPICKVAL(SBQQ__FilterField__c,&apos;&apos;)))</errorConditionFormula>
        <errorMessage>You may not specify both product and filter.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SBQQ__Filter_requires_operators</fullName>
        <active>true</active>
        <description>Operator must be entered when filter is specified.</description>
        <errorConditionFormula>AND(ISPICKVAL(SBQQ__Operator__c,&apos;&apos;),NOT(ISPICKVAL(SBQQ__FilterField__c,&apos;&apos;)))</errorConditionFormula>
        <errorMessage>Operator is required when Filter Field is specified.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SBQQ__Product_or_filter_required</fullName>
        <active>true</active>
        <description>Requires that product or filter be specified.</description>
        <errorConditionFormula>AND(ISBLANK(SBQQ__Product__c),ISPICKVAL(SBQQ__FilterField__c,&apos;&apos;))</errorConditionFormula>
        <errorMessage>You must specify either product or filter field</errorMessage>
    </validationRules>
