    <validationRules>
        <fullName>SBQQ__Ensure_rule_type_nested_level</fullName>
        <active>true</active>
        <description>Ensures the related Product Rule is of type Validation, Alert or Selection when specifying either a parent or child bundle condition level.</description>
        <errorConditionFormula>(TEXT(SBQQ__ProductRule__r.SBQQ__Type__c) != null &amp;&amp; TEXT(SBQQ__ProductRule__r.SBQQ__Type__c) != &apos;Validation&apos; &amp;&amp; TEXT(SBQQ__ProductRule__r.SBQQ__Type__c) != &apos;Alert&apos; &amp;&amp; TEXT(SBQQ__ProductRule__r.SBQQ__Type__c) != &apos;Selection&apos;) &amp;&amp; (TEXT(SBQQ__AscendingNestedLevel__c) != null || TEXT(SBQQ__DescendingNestedLevel__c) != null)</errorConditionFormula>
        <errorMessage>Condition Level only supported for Validation, Alert and Selection product rules.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SBQQ__Ensure_rule_type_nested_level_actions</fullName>
        <active>true</active>
        <description>Ensures the related Product Rule is of type Selection when specifying an action level.</description>
        <errorConditionFormula>(TEXT(SBQQ__ProductRule__r.SBQQ__Type__c) != null &amp;&amp; TEXT(SBQQ__ProductRule__r.SBQQ__Type__c) != &apos;Selection&apos;) &amp;&amp; (TEXT(SBQQ__DescendingActionNesting__c) != null)</errorConditionFormula>
        <errorMessage>Action Level only supported for Selection product rules.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SBQQ__Ensure_supported_nested_level</fullName>
        <active>true</active>
        <description>Ensure the value of either the parent or child bundle level field is None or 1.</description>
        <errorConditionFormula>(TEXT(SBQQ__AscendingNestedLevel__c) != null &amp;&amp; TEXT(SBQQ__AscendingNestedLevel__c) != &apos;1&apos;) || (TEXT(SBQQ__DescendingNestedLevel__c) != null &amp;&amp; TEXT(SBQQ__DescendingNestedLevel__c) != &apos;1&apos;) || (TEXT(SBQQ__DescendingActionNesting__c) != null &amp;&amp; TEXT(SBQQ__DescendingActionNesting__c) != &apos;1&apos;)</errorConditionFormula>
        <errorMessage>The maximum supported Bundle Level is 1.</errorMessage>
    </validationRules>
