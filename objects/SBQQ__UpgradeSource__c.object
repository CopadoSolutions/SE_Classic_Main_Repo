    <fields>
        <fullName>SBQQ__UpgradeConversionRate__c</fullName>
        <deprecated>false</deprecated>
        <description>The conversion rate between the upgrade product and the source product.</description>
        <externalId>false</externalId>
        <inlineHelpText>Specify a ratio for quantity conversion from Upgrade Source to Upgrade Product. e.g. &quot;5:1&quot; indicates that 5 units of Upgrade Source convert to 1 unit of Upgrade Product. If blank, no Upgrade Conversion Rate is enforced.</inlineHelpText>
        <label>Upgrade Conversion Rate</label>
        <length>100</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
