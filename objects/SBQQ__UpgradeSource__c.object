        <inlineHelpText>Specify the product to upgrade to.</inlineHelpText>
        <label>Upgrade Product</label>
        <referenceTo>Product2</referenceTo>
        <relationshipLabel>Upgrade Sources</relationshipLabel>
        <relationshipName>UpgradeSources</relationshipName>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Lookup</type>
