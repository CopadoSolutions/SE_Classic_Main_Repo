    <validationRules>
        <fullName>SBQQ__FilterCombinationValidation</fullName>
        <active>true</active>
        <description>Validation for the combination of Filter Type Variable.</description>
        <errorConditionFormula>AND(ISPICKVAL(SBQQ__FilterType__c, &quot;Variable&quot;), ISBLANK(SBQQ__FilterVariable__c))</errorConditionFormula>
        <errorMessage>A Filter Type of &apos;Variable&apos; must have a Filter Variable.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SBQQ__ScopeAndErrorConditionTestedObject</fullName>
        <active>true</active>
        <description>Validation for the combination of Product Scope and Tested Object.</description>
        <errorConditionFormula>OR(AND(ISPICKVAL(SBQQ__Rule__r.SBQQ__Scope__c,&quot;Quote&quot;),OR(ISPICKVAL(SBQQ__TestedObject__c, &quot;Product Option&quot;),ISPICKVAL(SBQQ__TestedObject__c, &quot;Configuration Attributes&quot;))),AND(ISPICKVAL(SBQQ__Rule__r.SBQQ__Scope__c,&quot;Product&quot;),OR(ISPICKVAL(SBQQ__TestedObject__c, &quot;Quote Line&quot;),ISPICKVAL(SBQQ__TestedObject__c, &quot;Quote Line Group&quot;))))</errorConditionFormula>
        <errorMessage>This combination of ProductRule.Scope and ErrorCondition.TestedObject is not valid.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SBQQ__Tested_attr_required_if_global_field</fullName>
        <active>true</active>
        <description>Tested Attribute required since the Tested Field is a global Configuration Attribute field.</description>
        <errorConditionFormula>IF(TEXT(SBQQ__TestedObject__c) == &apos;Configuration Attributes&apos; &amp;&amp; CONTAINS(TEXT(SBQQ__TestedField__c), &apos;.&apos;) &amp;&amp; SBQQ__TestedAttribute__c == &apos;&apos;, true, false)</errorConditionFormula>
        <errorMessage>Tested Attribute required since the Tested Field is a global Configuration Attribute field.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SBQQ__Tested_field_or_variable_required</fullName>
        <active>true</active>
        <description>Requires that either Tested Variable or Tested Field be specified.</description>
        <errorConditionFormula>AND(ISPICKVAL(SBQQ__TestedField__c,&quot;&quot;),ISBLANK(SBQQ__TestedVariable__c))</errorConditionFormula>
        <errorMessage>You must specify either Tested Field or Tested Variable.</errorMessage>
    </validationRules>
