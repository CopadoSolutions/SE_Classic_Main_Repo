    <validationRules>
        <fullName>SBQQ__FilterTypeScope</fullName>
        <active>true</active>
        <description>Validation for Filter type Product Rules not having a value of &apos;Product&apos; in the Scope field.</description>
        <errorConditionFormula>AND( 
ISPICKVAL(SBQQ__Type__c, &quot;Filter&quot;),
NOT(ISPICKVAL(SBQQ__Scope__c, &quot;Product&quot;))
)</errorConditionFormula>
        <errorMessage>The Scope field must point to &apos;Product&apos; for Filter type Product Rules.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SBQQ__ProductEvaluationEventValidation</fullName>
        <active>true</active>
        <description>Validation for Scope Quote having an Evaluation Event of &apos;Edit&apos; and &apos;Load&apos;</description>
        <errorConditionFormula>OR(AND(ISPICKVAL(SBQQ__EvaluationEvent__c, &quot;Edit&quot;), ISPICKVAL(SBQQ__Scope__c, &quot;Quote&quot;)), AND(ISPICKVAL(SBQQ__EvaluationEvent__c, &quot;Load&quot;), ISPICKVAL(SBQQ__Scope__c, &quot;Quote&quot;)))</errorConditionFormula>
        <errorMessage>Only rules with Scope &apos;Product&apos; can have Evaluation Events of type &apos;Edit&apos; or &apos;Load&apos;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SBQQ__RequiredEvaluationEvent</fullName>
        <active>true</active>
        <description>Validation for required evaluation events if scope is filled in.</description>
        <errorConditionFormula>AND(
            ISBLANK(TEXT(SBQQ__EvaluationEvent__c)),
            OR(ISPICKVAL(SBQQ__Scope__c, &quot;Quote&quot;), ISPICKVAL(SBQQ__Scope__c, &quot;Product&quot;))
            )</errorConditionFormula>
        <errorMessage>The Evaluation Event field must be populated with a value.</errorMessage>
    </validationRules>
