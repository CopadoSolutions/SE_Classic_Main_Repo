			var toast = document.querySelector('#toast');
			toast.addEventListener('click', function(e) {
				var close = e.currentTarget;
				close.parentElement.querySelector("#toast").classList.remove('visible');
			});
		}

		$(window).load( function() {
			$(window).scrollTop(0);
			// Logic for closing toasts
			toastListener();

			document.querySelector('.previewButton').setAttribute('type', 'button');


			// Logic for expanding fieldSetBody and fieldSetFooter
			var row1 = document.getElementById('row1');

			row1.addEventListener('click', function(e) {
				var fieldSet = getFieldSetIfInLegendOrFooter(e.target);
				if (!fieldSet ) {
					return;
				} else {
					var expanded = fieldSet.classList.contains('expanded');
					expandOrCollapseFieldSetBody(fieldSet, expanded);
					updateFieldSetFooter(fieldSet, expanded);
					fieldSet.classList.toggle('expanded');
					return;
				}
			});

			// Logic for tooltip of fieldSetBody
			row1.addEventListener('mouseover', function(e) {
				var toolTip = (e.target.nodeName == 'I') ? e.target : null;

				if (!toolTip) {
					return;
				}

				setToolTipPosition(e.target);

				return;
			});

			row1.addEventListener('mouseout', function(e) {
				var toolTip = (e.target.nodeName == 'I') ? e.target : null;

				if (!toolTip) {
					return;
				}

				var toolTipText = e.target.children[0];
				toolTipText.style.left = '';
				toolTipText.style.top = '';
				toolTipText.style.display = 'none';

				return;
			});

		}); // window.load();
		function expandOrCollapseFieldSetBody(fieldSet, expanded) {
			var fieldSetBody = fieldSet.querySelector('.fieldset-body');
			fieldSetBody.style.maxHeight = expanded ? "0px": "1500px";
			return;
		}

		function updateFieldSetFooter(fieldSet, expanded) {
			var fieldSetFooter = fieldSet.querySelector('footer');

			var chevron = fieldSetFooter.getElementsByTagName('i')[0],
				span    = fieldSetFooter.getElementsByTagName('span')[0];

			 chevron.style.transform = expanded ? "rotate(0deg)" : "rotate(180deg)";
			 span.textContent = expanded ? "{!JSENCODE($Label.lbl_show)}" : "{!JSENCODE($Label.lbl_hide)}";

		   return;
		}

		function getFieldSetIfInLegendOrFooter(target) {
			var targetParent      = target.parentElement,
				targetGrandparent = targetParent.parentElement;

			if (target.nodeName == 'FIELDSET') {
				return target;
			}

			if (targetParent.nodeName == 'FIELDSET') {
				return targetParent;
			}

			if (targetParent.classList.contains('expandable-parent')) {
				return targetParent.parentElement;
			}

			if (targetParent.classList.contains('expandable-child')) {
				return targetParent.parentElement.parentElement;
			}

			if (targetGrandparent.classList.contains('expandable-child')) {
				return targetGrandparent.parentElement.parentElement;
			}

			return null;
		}

		function setToolTipPosition(toolTip) {
			var currentNode = toolTip;
			var toolTipText          = toolTip.children[0];
			var toolTipPosition     = toolTip.getBoundingClientRect();
			var offsetParentPosition = null;

			while (currentNode.nodeName != 'FIELDSET') {
				currentNode = currentNode.parentElement;
			}

			currentNode.children[1].style.position;
			offsetParentPosition = currentNode.getBoundingClientRect();

			toolTipText.style.display = 'block';
			toolTipText.style.left = toolTipPosition.right - 15 + 'px';
			toolTipText.style.bottom =  offsetParentPosition.bottom - toolTipPosition.bottom - toolTipText.offsetHeight + 20 + 'px';
		}

		$("select option[value='']").remove()


	</script>
